<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".parkFragment">

    <!-- TODO: Update blank fragment layout -->


    <ImageView
        android:id="@+id/imageView33"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="215dp"
        android:layout_marginTop="-15dp"
        android:src="@drawable/ellipse_3" />

    <ImageView
        android:id="@+id/imageView34"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="25dp"
        android:layout_marginTop="-48dp"
        android:src="@drawable/ellipse_1" />

    <ImageView
        android:id="@+id/imageView35"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="0dp"
        android:layout_marginLeft="130dp"
        android:src="@drawable/ellipse_4" />

    <ImageView
        android:id="@+id/imageView37"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="290dp"
        android:layout_marginLeft="353dp"
        android:src="@drawable/subtract" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Parking Status"
        android:fontFamily="@font/avenir_black"
        android:textSize="36dp"
        android:layout_marginTop="140dp"
        android:layout_marginLeft="50dp"
        android:textColor="#575DFB"
        />

    <ImageView
        android:id="@+id/imageView38"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="220dp"
        android:layout_marginLeft="10dp"
        android:src="@drawable/parking" />



    <ImageButton
        android:id="@+id/imgUser"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/perfilrounded"
        android:contentDescription="@string/app_name"
        android:minWidth="48dp"
        android:minHeight="48dp"
        android:src="@drawable/perfil" />
    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="90dp"
        android:layout_marginTop="30dp"
        android:text="Bienvenido"
        android:textColor="#A9A9AA"
        android:fontFamily="@font/avenir_roman"
        android:textSize="12dp"

        />

    <TextView
        android:id="@+id/txtUser"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="90dp"
        android:layout_marginTop="45dp"
        android:text="Nombre de usuario"
        android:textColor="#19104E"
        android:textSize="16sp"
        android:fontFamily="@font/avenir_roman"
        />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/btnNotification"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="320dp"
        android:layout_marginTop="15dp"
        android:clickable="true"
        android:backgroundTint="#f5f5ff"
        app:borderWidth="0dp"
        android:contentDescription="@string/app_name"
        android:src="@drawable/notification" />

    <TextView
        android:id="@+id/coches"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="430dp"
        android:layout_marginLeft="60dp"
        android:text="Disponibles para coches:"
        android:textColor="#19104E"
        android:textSize="18dp"
        />

    <TextView
        android:id="@+id/bicis"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="460dp"
        android:layout_marginLeft="60dp"
        android:text="Disponibles para bicis:"
        android:textColor="#19104E"
        android:textSize="18dp"
        />

    <TextView
        android:id="@+id/motos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="490dp"
        android:layout_marginLeft="60dp"
        android:text="Disponibles para motos:"
        android:textColor="#19104E"
        android:textSize="18dp"
        />

</FrameLayout>